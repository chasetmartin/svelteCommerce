// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.sqlite"
}

model Window {
  id Int @id @default(autoincrement())
  inserttype Float
  window String
  width Float
  height Float
  price Float
  createdAt DateTime @default(now())
}